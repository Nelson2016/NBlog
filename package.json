{
  "name": "n-blog",
  "version": "1.0.0",
  "description": "Nelson's Blog",
  "main": "./views/app.js",
  "scripts": {
    "setup": "npm install && npm install cp-cli -g",
    "dev": "run-s clean start:dev",
    "prod": "run-s clean build:prod start:prod",
    "build:dev": "webpack --config ./webpack/webpack.dev.config.js --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js --progress",
    "clean": "rimraf ./dist/* && cp-cli favicon.ico dist && cp-cli ./asset/images/N-black.png ./dist/N-black.png",
    "start:dev": "cross-env NODE_ENV=development nodemon ./webpack/start.dev --watch ./dist --watch ./server",
    "start:prod": "node ./dist/server.bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nelson2016/NBlog.git"
  },
  "keywords": [
    "前端,博客,blog,nelson,node,react,koa"
  ],
  "author": "Nelson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nelson2016/NBlog/issues"
  },
  "homepage": "https://github.com/Nelson2016/NBlog#readme",
  "devDependencies": {
    "asset-require-hook": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "i": "^0.3.6",
    "isomorphic-style-loader": "^4.0.0",
    "koa": "^2.3.0",
    "koa-static": "^4.0.1",
    "koa-views": "^6.1.1",
    "koa-webpack-middleware": "^1.0.7",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm": "^5.5.1",
    "npm-run-all": "^4.1.1",
    "nunjucks": "^3.0.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "react-breadcrumbs": "^1.6.6"
  }
}
